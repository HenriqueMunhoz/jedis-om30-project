<%= form_with(model: @municipe, local: true) do |f| %>
<div class="card">
  <div class="card-body">

    <div class="mb-3">
      <%= f.label :full_name, { class: "form-label"} %>
      <%= f.text_field :full_name, { class: "form-control", placeholder: "Full name" } %>
    </div>

    <div class="mb-3">
      <%= f.label :cpf, { class: "form-label"} %>
      <%= f.text_field :cpf, { class: "form-control", placeholder: "Full name" } %>
    </div>

    <div class="mb-3">
      <%= f.label :cns, { class: "form-label"} %>
      <%= f.text_field :cns, { class: "form-control", placeholder: "CNS" } %>
    </div>

    <div class="mb-3">
      <%= f.label :email, { class: "form-label"} %>
      <%= f.email_field :email, { class: "form-control", placeholder: "email" } %>
    </div>

    <div class="mb-3">
      <%= f.label :birth_date, { class: "form-label"} %>
      <%= f.date_field :birth_date, { class: "form-control" } %>
    </div>

    <div class="mb-3">
      <%= f.label :phone, { class: "form-label"} %>
      <%= f.telephone_field :phone, { class: "form-control", placeholder: "telefone" } %>
    </div>

    <div class="mb-3">
      <%= f.label :photo, { class: "form-label"} %>
      <%= f.text_field :photo, { class: "form-control", placeholder: "Full name" } %>
    </div>

    <%= f.submit "Submit", class: "btn btn-primary" %>

  </div>
</div>
<% end %>