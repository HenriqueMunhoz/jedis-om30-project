<%= form_with(model: @municipe, local: true) do |form| %>
  <%= form.label :nome_completo %>
  <%= form.text_field :nome_completo %>

  <%= form.label :cpf %>
  <%= form.text_field :cpf %>

  <%= form.label :cns %>
  <%= form.text_field :cns %>

  <%= form.label :email %>
  <%= form.email_field :email %>

  <%= form.label :birth_day %>
  <%= form.date_field :birth_day %>

  <%= form.label :phone %>
  <%= form.telephone_field :phone %>

  <%= form.label :photo %>
  <%= form.file_field :photo %>

  <%= form.label :status %>
  <%= form.select :status, ['Ativo', 'Inativo'] %>

  <%= form.fields_for :address do |address_form| %>
    <%= address_form.label :zip_code %>
    <%= address_form.text_field :zip_code %>

    <%= address_form.label :street %>
    <%= address_form.text_field :street %>


    <%= address_form.label :neighborhood %>
    <%= address_form.text_field :neighborhood %>

    <%= address_form.label :city %>
    <%= address_form.text_field :city %>

    <%= address_form.label :state %>
    <%= address_form.text_field :state %>
  <% end %>

  <%= form.submit %>
<% end %>